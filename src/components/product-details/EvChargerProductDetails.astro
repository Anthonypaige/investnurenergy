---
import evChargers from '~/evchargerlist.json';
const productId = 1; // Replace this with the actual product ID you want to display
const product = evChargers.find(charger => charger.id === productId);
---

<article>
  <h1>{product.name}</h1>
  <p>Price: {product.price}</p>
  <p>Max Power Output: {product.maxPowerOutput}</p>
  <p>Wifi: {product.wifi}</p>
  <p>Bluetooth: {product.bluetooth}</p>
  <p>Chord Management: {product.chordManagement}</p>
  <p>Installation: {product.installation}</p>
  <p>Outdoor Rating: {product.outdoorRating}</p>
  <p>Energy Star Certified: {product.energyStarCertified}</p>
  <p>Chord Length: {product.chordLength}</p>
</article>
