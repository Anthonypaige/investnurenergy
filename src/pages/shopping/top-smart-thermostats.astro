title="Features Included"
subtitle="All thermostats in this table are Energy Star certified, Wi-Fi enabled and share these base features"
columns={3}
items={[
{
  title: 'Wi-Fi Enabled',
  description: 'Control your thermostat remotely via smartphone  for added convenience.',
  icon: 'tabler:wifi-2'
},
{
  title: 'Occupancy Sensing',
  description: 'Utilizes sensors and geofencing to reduce HVAC use when home is unoccupied.',
  icon: 'tabler:home-signal'
},
{
  title: 'Customized Scheduling',
  description: 'Set schedules to maximize energy savings with Time-of-Use rates.',
  icon: 'tabler:clock-dollar'
},
{
  title: 'Utility Program Sync',
  description: 'Works with utility programs for peak times and offers manual override.',
  icon: 'tabler:home-link'
},
{
  title: 'HVAC Energy Use Insights',
  description: 'Provides monthly run-time data to understand HVAC energy use.',
  icon: 'tabler:air-conditioning'
},
{
  title: 'Energy Insights',
  description: 'Gives real-time feedback and monthly data for energy-efficient choices.',
  icon: 'tabler:eye-bolt'
},
]}
/>


