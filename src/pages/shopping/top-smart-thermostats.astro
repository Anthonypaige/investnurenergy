<style>
  .checkmark {
 color: green;
 }
  .cross {
 color: red;
 }

 
</style>
  

<Layout metadata={metadata}>


<Hero actions={[]} videoSrc="/images/smart-thermostat-video.mp4">
<Fragment slot="title">
<div class="text-left text-3xl sm:text-4xl md:text-5xl lg:text-6xl font-bold leading-tight tracking-tighter mb-4 font-heading mp-15">
 <div>Smart Thermostats</div>
</div>
</Fragment>
</Hero>
    
    
<section class="container mx-auto p-9"></section>


