---
import Layout from '~/layouts/PageLayout.astro';
import Features2 from '~/components/widgets/Features2.astro';
import Hero from '~/components/widgets/Hero.astro';
import Button from '~/components/ui/Button.astro';

const metadata = {
  title: 'Portable Power Stations Comparison Table',
};

---
<style>
.checkmark {
color: green;
}
.cross {
color: red;
}
</style>

<Layout metadata={metadata}>
  <Hero actions={[]} videoSrc="/images/portable-power-video.mp4">
    <Fragment slot="title">
      <div class="text-right text-green-700 font-bold lg:text-6xl md:text-4xl sm:text-2xl text-xl">
        <div class="inline-block text-right">
          <div>Portable Power</div>
          <div class="text-center lg:text-6xl md:text-4xl sm:text-2xl text-xl" style="margin: 0 auto;">
            Stations
          </div>
        </div>
      </div>
    </Fragment>
  </Hero>

<section class="container mx-auto p-9"></section>


  <Features2
  title="Features Included"
  subtitle="All portable power stations in this table meet a minimum 3kWh capacity, and offer these features"
  columns={3}
  items={[
    {
     title: 'Min. 3kWh Capacity',
      description: 'Meets the 3kWh minimum capacity required for 30C tax credit eligibility.',
      icon: 'tabler:receipt-tax'
    },
    {
     title: 'Portable & Modular',
      description: 'Designed for on-the-go and home use, and easily expandable.',
      icon: 'tabler:walk'
    },
    {
     title: 'Energy Arbitrage',
     description: 'Allows you to capitalize on low-cost energy periods, saving money when rates soar.',
     icon: 'tabler:clock-dollar'
    },
    {
      title: 'Clean Energy Integration',
      description: 'Integrates with solar panels or wind turbines for clean energy use.',
      icon: 'tabler:sun-wind'
    },
    {
      title: 'Emergency Backup',
      description: 'Reliable power source for grid failures, offering grid independence',
      icon: 'tabler:urgent'
    },
    {
     title: 'User-Friendly Setup',
     description: 'Gives you the power to install it yourself, dodging costly professional fees.',
     icon: 'tabler:user-check'    
    }
  ]}
/>
   


<!-- Table -->
   
  <!-- Responsive Table Wrapper -->
<div class="flex flex-col overflow-x-auto ml-20 mr-20 ">
  <!-- Search Input -->
  <div class="relative m-[2px] mb-3 mr-5 float-left">
    <label for="inputSearch" class="sr-only">Search </label>
    <input id="inputSearch" type="text" placeholder="Search..." class="block w-64 rounded-lg border dark:border-none dark:bg-neutral-600 py-2 pl-10 pr-4 text-sm focus:border-blue-400 focus:outline-none focus:ring-1 focus:ring-blue-400" />
    <span class="pointer-events-none absolute left-3 top-1/2 -translate-y-1/2 transform">
      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="h-4 w-4 text-neutral-500 dark:text-neutral-200">
        <path stroke-linecap="round" stroke-linejoin="round" d="M21 21l-5.197-5.197m0 0A7.5 7.5 0 105.196 5.196a7.5 7.5 0 0010.607 10.607z" />
      </svg>
    </span>
